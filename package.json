{
  "name": "icojs",
  "description": "parse ico file",
  "version": "0.5.0",
  "author": "egy186",
  "bugs": {
    "url": "https://github.com/egy186/icojs/issues"
  },
  "dependencies": {
    "jimp": "^0.2.24"
  },
  "devDependencies": {
    "@egy186/eslint-config": "~0.23.0",
    "babel-core": "^6.23.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.9.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "~3.15.0",
    "istanbul": "^0.4.4",
    "jsdoc-to-markdown": "^3.0.0",
    "jstransformer-markdown-it": "^2.0.0",
    "markdown-include": "^0.4.3",
    "mocha": "^3.1.2",
    "npm-run-all": "^4.0.1",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.5.2",
    "webpack": "^2.2.1"
  },
  "engines": {
    "node": ">=4.2.3"
  },
  "files": [
    "dist",
    "src",
    "browser.js",
    "index.js"
  ],
  "homepage": "https://egy186.github.io/icojs",
  "keywords": [
    "ico",
    "parse"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/egy186/icojs.git"
  },
  "scripts": {
    "build": "npm-run-all jsdoc --parallel build:*",
    "build:dist": "webpack --config webpack.config.js",
    "build:docs": "pug templates/index.pug --obj package.json --out docs",
    "build:readme": "markdown-include readme.json",
    "clean": "rimraf coverage dist docs/index.html templates/documentation.md README.md",
    "jsdoc": "jsdoc2md src/ico.js --template templates/documentation.hbs > templates/documentation.md",
    "lint": "eslint .",
    "test": "istanbul cover --root src --report lcov --dir coverage node_modules/mocha/bin/_mocha -- test/*.js"
  }
}
